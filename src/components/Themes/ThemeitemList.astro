---
import Computer from '../Icons/Computer.astro';

interface Props {
  title: string;
  topics: string[];
}

const { title, topics } = Astro.props
---
<li>
  <h3 class='text-2xl font-bold p-4'>{title}</h3>
  <ul>
    {topics.map(topic => (<li class="flex gap-3 p-3">
      <div class="h-12">
        <Computer /> 
      </div>{topic}
    </li>))}
  </ul>
</li>
